@using System.Collections.Generic
@using WaFFLs.Generation
@using WaFFLs.Generation.Models
@inherits RazorEngine.Templating.TemplateBase<List<GameRecord>>

<html>
<head></head>
<body>

<h2>@ViewBag.Title</h2>
<i>@ViewBag.Summary</i>

<ol>
    @foreach (var x in Model)
    {
        <li>@x.Value @x.Team1.GetDisplayName() vs @x.Team2.GetDisplayName() @x.Game.GetDisplayName()</li>
    }
</ol>

</body>
</html>