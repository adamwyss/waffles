@using System.Collections.Generic
@using WaFFLs.Generation
@using WaFFLs.Generation.Models
@inherits RazorEngine.Templating.TemplateBase<List<StreakRecord>>

<html>
<head></head>
<body>

<h2>@ViewBag.Title</h2>

<ol>
    @foreach (var x in Model)
    {
        <li>@x.Team.GetDisplayName() @x.Value (@x.From.GetDisplayName() - @x.To.GetDisplayName())</li>
    }
</ol>

</body>
</html>